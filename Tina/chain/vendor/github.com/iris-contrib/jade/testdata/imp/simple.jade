:go:import "github.com/iris-contrib/jade/testdata/imp/model"

:go:func Simple( u *model.User, st []model.Story )


html
    body
        h1= u.FirstName
        p Here's a list of your favorite colors:
        ul
            each colorName in u.FavoriteColors
                li= colorName

        ul
            each story in st
                li= story.StoryId
                li= story.UserId
                li= story.UserName